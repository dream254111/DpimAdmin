@using Newtonsoft.Json;

 @*@{var auth = (ViewBag.Auth ?? null) as Dpim_Elearning.Models.Authentication.Authentication;
     
     var username = auth.username;}*@

<!DOCTYPE html>
<html lang="en" ng-app="master">
<head>
    <meta charset="utf-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap-theme.min.css")" rel="stylesheet" />

    <link rel="stylesheet" href="@Url.Content("~/Content/asset/font-awesome-4.6.3/css/font-awesome.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/ionicons.min.css")">
    <link rel="stylesheet" href='@Url.Content("~/Content/asset/dist/css/AdminLTE.min.css")' rel="stylesheet">
    <link rel="stylesheet" href='@Url.Content("~/Content/asset/dist/css/skins/_all-skins.min.css")' rel="stylesheet">
    <link rel="stylesheet" href="@Url.Content("~/Content/asset/datepicker/jquery.datetimepicker.css")">

    <!-- jQuery 2.1.4 -->
    <script src='@Url.Content("~/Content/asset/plugins/jQuery/jQuery-2.1.4.min.js")'></script>
    <script src="@Url.Content("~/Content/asset/datepicker/jquery.datetimepicker.js")"></script>

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    @RenderSection("header", required: false)

    <style class="css">
        .preloader {
            padding: 50px;
            background: url('@Url.Content("~/images/base/preloader.gif")') no-repeat 50% 50% transparent;
        }
        .content-wrapper, .right-side {

            background-color: #fff;
        }

    </style>
</head>

<body>
    <!-- Site wrapper -->
    <div class="navbar navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a style="margin-top:-10px;" href="@Url.Content("~/Home/Index")" class="logo navbar-brand">
                    <img src="~/image/tome1.png" class="navbar-admin-logo" />

                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Admin Manage", "AdminManage", "Home")</li>
                    <li>@Html.ActionLink("Send Email", "SendingEmail", "Home")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                    @*<li v-show="@ViewBag.auth.isAuthenticated">@Html.ActionLink("UpdateStructure", "UpdateStructure", "Setting")</li>*@

                </ul>
                @*<div class="navbar-form navbar-right">
                    <label style="color:white">@auth.username</label>*@
                <ul class="nav navbar-nav navbar-right">
                    <li> <a href="#" >@ViewBag.auth.username</a></li>
                    @*<li v-show="@auth.isAuthenticated">
                        @Html.ActionLink("Sign Out", "Sign Out", "Home")
                    </li>*@
                </ul>

                @*</div>*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div class="wrapper">

            @RenderBody()

            <hr />
        </div>

        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <!-- SlimScroll -->
    <script src='@Url.Content("~/Content/asset/plugins/slimScroll/jquery.slimscroll.min.js")'></script>
    <!-- FastClick -->
    <script src='@Url.Content("~/Content/asset/plugins/fastclick/fastclick.js")'></script>
    <!-- AdminLTE App -->
    <script src='@Url.Content("~/Content/asset/dist/js/app.min.js")'></script>
    <script src='@Url.Content("~/scripts/bootstrap.js")'></script>
    <script src='@Url.Content("~/scripts/angular.min.js")'></script>
    <script src="~/scripts/toastr.js"></script>
    <link href="~/Content/toastr.css" rel="stylesheet" />
    @RenderSection("scripts", required: false)
</body>
</html>